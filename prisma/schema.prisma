generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  nome  String  @db.VarChar(128)
  email String  @unique @db.VarChar(255)
  phone String  @db.VarChar(20)

  @@map("users")
}